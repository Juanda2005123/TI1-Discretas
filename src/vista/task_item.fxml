<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="498.0" spacing="5.0" styleClass="light-black-background" stylesheets="@../values/styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="model.Task_itemController">
   <children>
      <Button mnemonicParsing="false" styleClass="buttonRound" stylesheets="@../values/styles.css">
         <graphic>
            <ImageView fx:id="editButtonImg" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/edit.png" />
               </image>
            </ImageView>
         </graphic>
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </Button>
      <Label fx:id="title" contentDisplay="CENTER" prefHeight="29.0" prefWidth="203.0" text="Integradora Discretas" textFill="#e1e1e1">
         <font>
            <Font name="Open Sans Regular" size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="deadLine" contentDisplay="CENTER" prefHeight="29.0" prefWidth="111.0" text="28-09-2023" textFill="#e1e1e1">
         <font>
            <Font name="Open Sans Regular" size="14.0" />
         </font>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label fx:id="prioritary" contentDisplay="CENTER" prefHeight="29.0" prefWidth="95.0" text="Prioritario" textFill="#e1e1e1">
         <font>
            <Font name="Open Sans Regular" size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin>
      </Label>
   </children>
</HBox>
